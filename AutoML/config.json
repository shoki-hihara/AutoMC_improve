{
  "exp_name": "AutoML for Model Compression Scheme - Fast Trial",
  "logging_dir": "../SLOG/",

  "cuda": true,
  "seed": 0,

  "our_kmodel_epochs": 20,                // 学習エポック数を大幅に減らす（例: 200 → 20）
  "our_kmodel_batch_size": 64,
  "our_kmodel_learning_rate": 0.001,

  "our_pmodel_batch_size": 128,           // バッチサイズも少し減らす
  "our_pmodel_avg_ratio": 0.3,
  "our_pmodel_learning_rate": 0.001,
  "our_pmodel_update_frequency": 1,
  "our_pmodel_optimal_num": 5,            // 最適候補数を半減（10 → 5）
  "our_pmodel_update_batch_num": 3,       // 更新バッチ数を減らす

  "evolution_population_num": 20,         // 集団数を減らして1世代の計算負荷軽減

  "rl_learning_rate": 0.00035,
  "rl_ema_baseline_decay": 0.95,

  "max_calg_num": 5,                      // 最大繰り返し回数を減らす（例: 10 → 5）

  "task_name": "resnet56+mini_cifar10+0.3",
  "task_info": {
    "class_num": 10,
    "image_size": 32,
    "image_channel": 3,
    "avg_data_num_per_class": 5000,
    "top1_acc": 0.93,
    "parameter_amount": 850000,
    "flops_amount": 125000000
  },

  "pretrain_epochs": 10,                  // 事前学習のエポック数も減らす

  "automl_search_time(h)": 1             // 探索時間を1時間に短縮（120 → 1）
}
